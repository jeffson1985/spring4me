<?xml version="1.0" encoding="UTF-8"?>
<sitemap xmlns="http://www.opensourceforce.org/schema/navigation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opensourceforce.org/schema/navigation http://www.opensourceforce.org/schema/spring4me-navigation.xsd">
	
	<navigation id="index" path="/index">
		<event on="do-login" to="index" />
		<event on="login-success" to="loginSuccess" />
		<event on="login-error" to="loginError" />
	</navigation>
	
	<navigation id="loginSuccess" path="/login-success">
	</navigation>
	
	<navigation id="loginError" path="/login-error">
	</navigation>
	
	<!-- flow A test -->
	<navigation id="flowA_step1" path="/flow/flowA_step1">
		<event on="next-step" to="flowA_step2"></event>
		<event on="process-flow" to="flowA_step2"></event>
	</navigation>
	<navigation id="flowA_step2" path="/flow/flowA_step2">
		<event on="prev-step" to="flowA_step1"></event>
		<event on="next-step" to="flowA_step3"></event>
		<event on="subflow" to="flowB_step1"></event>
	</navigation>
	<navigation id="flowA_step3" path="/flow/flowA_step3">
		<event on="prev-step" to="flowA_step2"></event>
		<event on="next-step" to="flowA_step1"></event>
	</navigation>
	
	<!-- flow B test -->
	<navigation id="flowB_step1" path="/flow/flowB_step1">
		<event on="next-step" to="flowB_step2"></event>
	</navigation>
	<navigation id="flowB_step2" path="/flow/flowB_step2">
		<event on="prev-step" to="flowB_step1"></event>
		<event on="next-step" to="flowB_step3"></event>
	</navigation>
	<navigation id="flowB_step3" path="/flow/flowB_step3">
		<event on="prev-step" to="flowB_step2"></event>
	</navigation>
	
</sitemap>